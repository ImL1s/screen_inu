{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "global-shortcut:default",
    "updater:default",
    "dialog:default",
    "process:default",
    "core:window:allow-hide",
    "core:window:allow-show",
    "core:window:allow-set-fullscreen",
    "core:window:allow-set-focus"
  ]
}